<keymap version="1" name="Windows copy Proper Redo" parent="$default">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="ActivateAndroidEmulatorToolWindow">
    <keyboard-shortcut first-keystroke="alt 3" />
  </action>
  <action id="ActivateLogcatToolWindow">
    <keyboard-shortcut first-keystroke="alt 6" />
  </action>
  <action id="ActivateTODOToolWindow" />
  <action id="ActivateTerminalToolWindow">
    <keyboard-shortcut first-keystroke="ctrl back_quote" />
  </action>
  <action id="Back">
    <mouse-shortcut keystroke="button4" />
  </action>
  <action id="CloseAllUnpinnedEditors">
    <keyboard-shortcut first-keystroke="ctrl f2" />
  </action>
  <action id="CloseContent">
    <keyboard-shortcut first-keystroke="ctrl w" />
  </action>
  <action id="EditorDeleteLine" />
  <action id="EditorScrollLeft">
    <keyboard-shortcut first-keystroke="ctrl alt left" />
  </action>
  <action id="EditorScrollRight">
    <keyboard-shortcut first-keystroke="ctrl alt right" />
  </action>
  <action id="EditorSelectWord">
    <keyboard-shortcut first-keystroke="ctrl f4" />
  </action>
  <action id="Forward">
    <mouse-shortcut keystroke="button5" />
  </action>
  <action id="Generate">
    <keyboard-shortcut first-keystroke="alt g" />
  </action>
  <action id="JumpToLastWindow">
    <keyboard-shortcut first-keystroke="shift escape" />
  </action>
  <action id="QuickChangeScheme" />
  <action id="SelectAllOccurrences">
    <keyboard-shortcut first-keystroke="shift ctrl alt s" />
  </action>
  <action id="ShowProjectStructureSettings">
    <keyboard-shortcut first-keystroke="shift ctrl alt j" />
  </action>
  <action id="SplitVertically">
    <keyboard-shortcut first-keystroke="alt v" />
  </action>
  <action id="Stop">
    <keyboard-shortcut first-keystroke="shift f5" />
  </action>
  <action id="UpdateRunningApplication" />
  <action id="android.deploy.ApplyChanges">
    <keyboard-shortcut first-keystroke="ctrl f10" />
  </action>
  <action id="android.deploy.CodeSwap">
    <keyboard-shortcut first-keystroke="ctrl alt f10" />
  </action>
</keymap>